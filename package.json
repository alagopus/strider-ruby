{
  "name": "strider-ruby",
  "version": "0.0.5",
  "description": "Ruby support for Strider Continuous Deployment",
  "scripts": {
    "test": "jshint && mocha -R tap"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelmior/strider-ruby.git"
  },
  "keywords": [
    "strider",
    "ruby"
  ],
  "strider": {
    "type": "job",
    "id": "ruby",
    "title": "Ruby",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": true
  },
  "author": "Michael Mior",
  "license": "BSD",
  "devDependencies": {
    "chai": "^4.2.0",
    "jshint": "^2.10.3",
    "mocha": "^5.2.0"
  }
}
